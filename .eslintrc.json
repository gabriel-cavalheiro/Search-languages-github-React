{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "import/prefer-default-export": "off",
      "react/react-in-jsx-scope": "off",
      "max-len": "off",
      "react/jsx-filename-extension": "off",
      "react/prop-types": "off"

    }
}
